{
  "name": "@jaxa/core",
  "version": "1.0.0-alpha.1",
  "description": "The base core of just another express application framework",
  "keywords": [
    "express",
    "framework",
    "typescript",
    "sequelize",
    "swagger"
  ],
  "author": "Daniel Pfisterer <daniel.pfisterer@power.cloud>",
  "homepage": "https://github.com/pure180/jaxa#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pure180/jaxa.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --inspect",
    "jest": "jest --forceExit --detectOpenHandles",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "test:watch": "cross-env ENV_FILE=./.env.test jest --watch",
    "test": "cross-env ENV_FILE=./.env.test jest",
    "version": "git add -A src"
  },
  "bugs": {
    "url": "https://github.com/pure180/jaxa/issues"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "openapi-types": "^8.0.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.1.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/hpp": "^0.2.1",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/sequelize": "^4.28.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jshint": "^2.12.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  }
}
