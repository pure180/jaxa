{
  "name": "@jaxa/cli",
  "version": "1.0.0-alpha.1",
  "description": "The basic jaxa cli",
  "author": "Daniel Pfisterer <daniel.pfisterer@power.cloud>",
  "homepage": "https://github.com/pure180/jaxa/blob/make-it-lerna/packages/cli#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "@jaxa/cli": "bin/jaxa",
    "jaxa": "bin/jaxa"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pure180/jaxa.git"
  },
  "scripts": {
    "build": "tsc",
    "jest": "jest --forceExit --detectOpenHandles",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "test:watch": "cross-env ENV_FILE=./.env.test jest --watch",
    "test": "cross-env ENV_FILE=./.env.test jest",
    "version": "git add -A src"
  },
  "bugs": {
    "url": "https://github.com/pure180/jaxa/issues"
  },
  "dependencies": {
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@types/esm": "^3.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jshint": "^2.12.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  }
}
