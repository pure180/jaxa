{
  "name": "@jaxa/cli",
  "version": "1.0.0-alpha.5",
  "description": "The basic jaxa cli",
  "author": "Daniel Pfisterer <daniel.pfisterer@power.cloud>",
  "keywords": [],
  "homepage": "https://github.com/pure180/jaxa/blob/make-it-lerna/packages/cli#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "@jaxa/cli": "bin/jaxa",
    "jaxa": "bin/jaxa"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pure180/jaxa.git"
  },
  "scripts": {
    "build": "tsc",
    "jest": "jest --forceExit --detectOpenHandles --passWithNoTests",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "test:watch": "cross-env ENV_FILE=./.env.test jest --watch",
    "test": "cross-env ENV_FILE=./.env.test jest",
    "version": "git add -A src",
    "watch": "tsc --watch"
  },
  "bugs": {
    "url": "https://github.com/pure180/jaxa/issues"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cli-select": "^1.1.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "esm": "^3.2.25",
    "minimist": "^1.2.5",
    "prompt": "^1.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/esm": "^3.2.0",
    "@types/jest": "^26.0.22",
    "@types/minimist": "^1.2.1",
    "@types/morgan": "^1.9.2",
    "@types/prompt": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jshint": "^2.12.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "gitHead": "8a2640571c7e332c07d5955f7e5a60ab92d851c4"
}
